{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://lwhqqhlvmtbcugzasamf.supabase.co/functions/v1/sitemap"
    },
    {
      "source": "/qr/:code",
      "destination": "https://lwhqqhlvmtbcugzasamf.supabase.co/functions/v1/qr-redirect/:code"
    },
    {
      "source": "/((?!.*\\.(jpg|jpeg|png|svg|ico|webp|gif|css|js|json|xml|txt|woff|woff2|ttf|eot)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
