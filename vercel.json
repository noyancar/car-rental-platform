{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://lwhqqhlvmtbcugzasamf.supabase.co/functions/v1/sitemap"
    },
    {
      "source": "/qr/:code",
      "destination": "https://lwhqqhlvmtbcugzasamf.supabase.co/functions/v1/qr-redirect/:code"
    },
    {
      "source": "/",
      "destination": "/api/inject-meta-tags/"
    },
    {
      "source": "/:path((?!assets|api|favicon|og-).*)",
      "destination": "/api/inject-meta-tags/:path"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
