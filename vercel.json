{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://lwhqqhlvmtbcugzasamf.supabase.co/functions/v1/sitemap"
    },
    {
      "source": "/qr/:code",
      "destination": "https://lwhqqhlvmtbcugzasamf.supabase.co/functions/v1/qr-redirect/:code"
    },
    {
      "source": "/blog/:slug",
      "destination": "/api/inject-meta-tags"
    },
    {
      "source": "/blog",
      "destination": "/api/inject-meta-tags"
    },
    {
      "source": "/cars/:id",
      "destination": "/api/inject-meta-tags"
    },
    {
      "source": "/cars",
      "destination": "/api/inject-meta-tags"
    },
    {
      "source": "/deals",
      "destination": "/api/inject-meta-tags"
    },
    {
      "source": "/how-it-works",
      "destination": "/api/inject-meta-tags"
    },
    {
      "source": "/nyncars",
      "destination": "/api/inject-meta-tags"
    },
    {
      "source": "/",
      "destination": "/api/inject-meta-tags"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
